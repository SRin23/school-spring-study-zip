<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
 <mapper namespace="joinln">
 	<insert id="insert_data" parameterType="kr.hs.study.dto.MyBatisSignupBean">
 		insert into joinln values (#{userId}, #{userPass}, #{userName}, #{userEmail}, #{userTel})
 	</insert>
 
 	<select id="select_data" resultType="kr.hs.study.dto.MyBatisSignupBean">
 		select * from joinln
 	</select>
</mapper>